/*begin content */
/*begin menu */
div.menu_search
{
	overflow: hidden;
	width: 978px;
}
div.menu_search div.menu
{
	float: left;
}
div.menu_search div.menu ul
{
	overflow: hidden;
	padding: 0px 0px 0px 7px;
}
div.menu_search div.menu ul li
{
	float: left;
	font-size: 12px;
	line-height: 18px;
	margin: 1px 0px 0px 0px;
	padding: 0px 1px 0px 1px;
	text-align: center;
	width: 125px;
}
div.menu_search div.menu ul li a
{
	background: url("../images/tab.png") no-repeat;
	color: #0A7285;
	cursor: pointer;
	display: block;
	float: left;
	padding: 7px 0px 5px 0px;
	position: relative;
	width: 125px;
}
div.menu_search div.menu ul li a
{
	text-decoration: none;
	text-transform: uppercase;
}
div.menu_search div.menu ul li.active
{
	height: 18px;
	margin: 0px;
	width: 128px;
}
div.menu_search div.menu ul li.active a
{
	background: url("../images/tab_act.gif")no-repeat;
	color: #0A7285;
	height: 18px;
	padding-top: 8px;
	width: 128px;
}
div.menu_search div.menu ul li.active a:hover
{
	background: url("../images/tab_act.gif") no-repeat !important;
	width: 128px;
}
div.menu_search div.menu ul li a:hover
{
	background: url("../images/tab_act_hover.gif")no-repeat;
	width: 125px;
}
div.menu_search div.search
{
	float: left;
	padding: 2px 0 0 17px;
}
div.menu_search div.search div.form
{
	background: url("../images/search.png")no-repeat 0 0;
	float: left;
	width: 170px;
	height: 20px;
}
div.menu_search div.search input.field
{
	float: left;
	background: none;
	border: none;
	width: 135px;
	padding: 2px 0 0 10px;
	font: 12px Arial,Tahoma,sans-serif;
	color: #0a7285;
	position: relative;
}
div.menu_search div.search input.submit
{
	float: left;
	background: url(../images/magnifer.gif) no-repeat 0 0;
	border: none;
	display: inline;
	width: 16px;
	height: 13px;
	padding: 0;
	margin: 3px 0 0 3px;
	cursor: pointer;
}
/*end menu */

.content_holder
